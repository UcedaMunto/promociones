{% extends 'base.html.twig' %}

{% block title %}Edicion de usuario{% endblock %}

{% block body %}

<div class="col-md-12" >
    <div class="row">
        <div class="col-md-4">
        <a href="{{ path('persona_cliente_index') }}">Lista de Clientes</a>
        </div>
        <div class="col-md-4">
            <a href="{{ path('persona_cliente_corregir_importador', {'id': persona_cliente.id}) }}"  target="_blank">Corregir imp</a>
            <p> 
                {% for importador in importadores %}
                    {% if importador != null %}
                        {{ importador.buyer }} - {{ importador.nombreFactura }} - {{ importador.estado.nombre }}   </br>
                    {% endif %}
                {% endfor %}
            </p>
        </div>
    </div>
    <div id="formPersonaCambio">
        {{ include('persona_cliente/form_edit.html.twig') }}
    </div>
            
            
</div>
 
    
        


        
{% endblock %}

{% block js_general %}
    <script src="{{ asset('js/personaClienteNew.js') }}" ></script>
{% endblock %}
